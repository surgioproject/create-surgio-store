{
  "name": "create-surgio-store",
  "version": "3.0.0",
  "description": "Surgio store starter kit",
  "bin": {
    "create-surgio-store": "./index.js"
  },
  "files": [
    "template",
    "createSurgioStore.js",
    "index.js",
    "README.md"
  ],
  "scripts": {
